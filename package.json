{
  "name": "candlelight-ageing-interface",
  "author": "Candlelight systems",
  "license": "MIT",
  "description": "Front-end interface for the Candlelight ageing systems",
  "version": "0.0.1",
  "homepage": "http://candlelight-systems.com",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-minify": "^0.4.3",
    "babel-plugin-transform-node-env-inline": "^0.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bootstrap": "^3.3.7",
    "electron-builder": "^19.19.1",
    "grunt-babel": "^7.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-webpack": "^2.0.1",
    "jquery": "^3.5.0",
    "node-jsgraph": "^1.16.29",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "bootstrap-slider": "^9.9.0",
    "fix-path": "^2.1.0",
    "grunt": "^1.0.1",
    "html-pdf": "^2.1.0",
    "lodash.debounce": "^4.0.8",
    "pdfkit": "^0.8.3",
    "shell": "^0.3.2"
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.candlelight.ageing",
    "productName": "Candlelight ageing",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools"
    },
    "asar": true,
    "asarUnpack": [
      "util/",
      "config.json"
    ],
    "files": [
      "app/",
      "main.js",
      "package.json",
      "environment.json",
      "config.json",
      "util/"
    ],
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "win": {
      "target": "portable"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 120
        },
        {
          "x": 310,
          "y": 120,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "x": 400,
        "y": 100,
        "width": 540,
        "height": 420
      }
    }
  }
}
